extends layout

block weatherContent
    .col-md-5
        .well.well-lg
            .row
                .col-sm-12 Location: #{weather.location}
                .col-sm-12 Weather: #{weather.weather}
                .col-sm-12 Temp: #{weather.temp}
block content
    .container
      .row
        .col-md-12
          .panel
            .panel-body
                  h4 Here are the restaurants that are close to the selected business 
                    b #{name}
                - var i = 0;
                - each r in resturants 
                  .row
                    br
                    .col-md-2.col-sm-3.text-center 
                        a.story-title(href='#')
                        img.img-circle(alt='', src='data:image/jpeg;base64,', style='width:100px;height:100px')            
                    .col-md-10.col-sm-9
                        h3=r.name
                        .row
                            .col-xs-9
                                h4
                                    a(href="#{r.url}", target='_blank')
                                        span.label.label-default Yelp Info 
                                h4
                                medium.text-muted(style="font-family:courier,'new courier';") Zip: #{r.zip}  &bull; 
                                medium.text-muted(style="font-family:courier,'new courier';") Rating: #{r.rating} 
                                -each d in directions
                                    -if (d != null && d.name == r.name)
                                        br
                                        medium.text-muted(style="font-family:courier,'new courier';") Miles: #{d.miles}
                                        br 
                                        medium.text-muted(style="font-family:courier,'new courier';") Mode: #{d.mode}  &bull;
                                        medium.text-muted(style="font-family:courier,'new courier';") Travel Time: #{d.travelTime}  
                                -each dr in drivingDirections
                                    -if (dr != null && dr.name == r.name)
                                        br
                                        medium.text-muted(style="font-family:courier,'new courier';") Mode: #{dr.mode}  &bull;
                                        medium.text-muted(style="font-family:courier,'new courier';") Travel Time: #{dr.travelTime}  
                            .col-xs-3
                            br
                   hr      
                  
              // /stories
              a.btn.btn-primary.pull-right.btnNext(href='/') More 
                i.glyphicon.glyphicon-chevron-right
        // /col-12
